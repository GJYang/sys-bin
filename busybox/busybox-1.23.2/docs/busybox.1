\&        \-Q,\-\-quiet\-output               No normal output
\&        \-s,\-\-shell=SHELL                Set shell quoting conventions
\&        \-T,\-\-test                       Version test (exits with 4)
\&        \-u,\-\-unquoted                   Don\*(Aqt quote output
.Ve
.Sp
Example:
.Sp
O=`getopt \-l bb: \*(-- ab:c:: \*(L"$@\*(R"` || exit 1
eval set \*(-- \*(L"$O\*(R"
while true; do
	case \*(L"$1\*(R" in
	\-a)	echo A; shift;;
	\-b|\-\-bb) echo \*(L"B:'$2'\*(R"; shift 2;;
	\-c)	case \*(L"$2\*(R" in
		"\*(L")	echo C; shift 2;;
		*)	echo \*(R"C:'$2'"; shift 2;;
		esac;;
	\-\-)	shift; break;;
	*)	echo Error; exit 1;;
	esac
done
